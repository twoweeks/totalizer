{
  "name": "twg-totalizer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "biome check",
    "lint:only-errors": "biome check --diagnostic-level=error",
    "prepare": "/bin/sh -c 'if type git >/dev/null 2>&1; then git config core.hooksPath .git-hooks; else true; fi'"
  },
  "dependencies": {
    "clsx": "2.1.1",
    "es-toolkit": "1.39.9",
    "preact": "10.27.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@preact/preset-vite": "2.10.2",
    "sass": "1.90.0",
    "typescript": "5.9.2",
    "vite": "7.1.2"
  }
}
